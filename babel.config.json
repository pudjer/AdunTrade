{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript",
    
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", {"decoratorsBeforeExport": true} ],
    ["i18next-extract", {"outputPath": "public/locales/{{locale}}/{{ns}}.json", "keyAsDefaultValue": true, "locales": ["en", "ru"]}],
  ]
}
